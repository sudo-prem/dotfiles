[
  // Suggestions Navigation
  {
    "command": "selectNextSuggestion",
    "key": "ctrl+j",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },
  {
    "command": "selectPrevSuggestion",
    "key": "ctrl+k",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus"
  },

  // Quick Open Navigation
  {
    "command": "workbench.action.quickOpenSelectNext",
    "key": "ctrl+j",
    "when": "inQuickOpen"
  },
  {
    "command": "workbench.action.quickOpenSelectPrevious",
    "key": "ctrl+k",
    "when": "inQuickOpen"
  },

  // Search Navigation
  {
    "command": "search.focus.nextInputBox",
    "key": "ctrl+j",
    "when": "inSearchEditor && inputBoxFocus || inputBoxFocus && searchViewletVisible"
  },
  {
    "command": "search.focus.previousInputBox",
    "key": "ctrl+k",
    "when": "inSearchEditor && inputBoxFocus || inputBoxFocus && searchViewletVisible && !searchInputBoxFocus"
  },

  // Editor Folding
  {
    "command": "editor.fold",
    "key": "ctrl+[",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "command": "editor.unfold",
    "key": "ctrl+]",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "command": "editor.foldAll",
    "key": "cmd+[",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "command": "editor.unfoldAll",
    "key": "cmd+]",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "command": "editor.foldRecursively",
    "key": "ctrl+shift+[",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "command": "editor.unfoldRecursively",
    "key": "ctrl+shift+]",
    "when": "editorTextFocus && foldingEnabled"
  },

  // List Navigation
  {
    "command": "list.focusDown",
    "key": "j",
    "when": "listFocus && !inputFocus"
  },
  {
    "command": "list.focusUp",
    "key": "k",
    "when": "listFocus && !inputFocus"
  },

  // Editor Navigation
  {
    "command": "workbench.action.navigateUp",
    "key": "ctrl+k",
    "when": "!suggestWidgetVisible && editorFocus"
  },
  {
    "command": "workbench.action.navigateDown",
    "key": "ctrl+j",
    "when": "!suggestWidgetVisible && editorFocus"
  },
  {
    "command": "workbench.action.navigateLeft",
    "key": "ctrl+h"
  },
  {
    "command": "workbench.action.navigateRight",
    "key": "ctrl+l"
  },

  // View Size Adjustments
  {
    "command": "workbench.action.increaseViewSize",
    "key": "ctrl+shift+=",
    "when": "editorFocus"
  },
  {
    "command": "workbench.action.decreaseViewSize",
    "key": "ctrl+shift+-",
    "when": "editorFocus"
  },

  // Zoom
  {
    "command": "workbench.action.zoomReset",
    "key": "cmd+0"
  },

  // Notifications
  {
    "command": "notifications.hideToasts",
    "key": "escape",
    "when": "notificationToastsVisible"
  },

  // Vim Mode Keybindings
  {
    "command": "workbench.action.showAllEditors",
    "key": "space f b",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus)"
  },
  {
    "command": "workbench.action.quickOpen",
    "key": "space space",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus)"
  },
  {
    "command": "workbench.action.findInFiles",
    "key": "space /",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus)"
  },
  {
    "command": "editor.action.codeAction",
    "key": "space c a",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.action.rename",
    "key": "space r n",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "editor.action.moveLinesUpAction",
    "key": "shift-k",
    "when": "vim.mode == 'VisualLine' && editorTextFocus"
  },
  {
    "command": "editor.action.moveLinesDownAction",
    "key": "shift-j",
    "when": "vim.mode == 'VisualLine' && editorTextFocus"
  },
  {
    "command": "editor.action.showHover",
    "key": "shift-k",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "leap.find",
    "key": "f",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },

  // File Explorer
  {
    "command": "explorer.newFile",
    "key": "a",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "command": "renameFile",
    "key": "r",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "command": "filesExplorer.copy",
    "key": "c",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "command": "filesExplorer.paste",
    "key": "p",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "command": "deleteFile",
    "key": "d",
    "when": "filesExplorerFocus && !inputFocus"
  },
  {
    "command": "explorer.openToSide",
    "key": "s",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },

  // Debugging Commands
  {
    "command": "workbench.action.debug.selectandstart",
    "key": "space d a",
    "when": "vim.mode == 'Normal' && (editorTextFocus || !inputFocus) && debuggersAvailable"
  },
  {
    "command": "workbench.action.debug.stop",
    "key": "space d t",
    "when": "vim.mode == 'Normal' && editorTextFocus && inDebugMode && !focusedSessionIsAttached"
  },
  {
    "command": "editor.debug.action.toggleBreakpoint",
    "key": "space d b",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },

  // Sidebar and Window Management
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "\\",
    "when": "sideBarFocus"
  },
  {
    "command": "workbench.files.action.focusFilesExplorer",
    "key": "\\",
    "when": "vim.mode == 'Normal' && editorTextFocus"
  },
  {
    "command": "aichat.newchataction",
    "key": "ctrl+\\",
    "when": "vim.mode != 'Insert' && editorTextFocus"
  },
  {
    "command": "workbench.action.toggleAuxiliaryBar",
    "key": "ctrl+\\",
    "when": "!editorTextFocus"
  },

  // Editor Splitting
  {
    "command": "workbench.action.splitEditorRight",
    "key": "space s v",
    "when": "vim.mode == 'Normal'"
  },
  {
    "command": "workbench.action.splitEditorDown",
    "key": "space s h",
    "when": "vim.mode == 'Normal'"
  },

  // Editor Navigation
  {
    "command": "workbench.action.nextEditor",
    "key": "right",
    "when": "vim.mode == 'Normal'"
  },
  {
    "command": "workbench.action.previousEditor",
    "key": "left",
    "when": "vim.mode == 'Normal'"
  },

  // Text Expansion
  {
    "command": "expand_region",
    "key": "ctrl+space",
    "when": "editorTextFocus"
  },
  {
    "command": "undo_expand_region",
    "key": "ctrl+shift+space",
    "when": "editorTextFocus && editorHasSelection"
  },

  // Console Logging
  {
    "command": "turboConsoleLog.displayLogMessage",
    "key": "cmd+l"
  }
]

